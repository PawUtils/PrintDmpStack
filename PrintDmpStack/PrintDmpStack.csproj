<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <RootNamespace>DmpStack</RootNamespace>
    <TargetFramework>net10.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <PublishAot>true</PublishAot>
    <PublishTrimmed>true</PublishTrimmed>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="DumbPrograms.DbgEng" Version="1.1.2025.929" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DmpStack\DmpStack.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="params.txt" CopyToOutputDirectory="PreserveNewest" />
    <None Include="symsrv.yes" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>

</Project>
